{
  "expo": {
    "name": "woodworker",
    "slug": "woodworker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.woodworker.app",
      "associatedDomains": [
        "applinks:woodworkcrafts.com",
        "applinks:app.woodworkcrafts.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.woodworker.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "woodworker",
            "host": "payment-success"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*.woodworkcrafts.com",
              "pathPrefix": "/payment-success"
            },
            {
              "scheme": "https",
              "host": "woodworkcrafts.com",
              "pathPrefix": "/payment-success"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "woodworker",
            "host": "service-pack-success"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "woodworker",
            "host": "wallet-topup-success"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "woodworkcrafts",
            "host": "payment-success"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "scheme": "woodworkcrafts",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "imgbbApiKey": "6baa7400e5d6030826c0332a84daea77"
    }
  }
}
